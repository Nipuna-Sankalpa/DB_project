<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FriendsTestBundle\Entity\EducationalInstitute" table="educational_institute">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" length="20" nullable="false"/>
    <field name="name" type="string" column="name" length="30" nullable="false"/>
    <field name="city" type="string" column="city" length="30" nullable="false"/>
    <field name="country" type="string" column="country" length="30" nullable="false"/>
  </entity>
</doctrine-mapping>
